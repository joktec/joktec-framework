{
  "name": "@baotg/graphql",
  "description": "Jobhopin - GraphQL Core libraries",
  "version": "0.0.4",
  "private": false,
  "license": "UNLICENSED",
  "author": "JobHopin",
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "dist/index",
  "types": "dist/index",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "purge": "rm -rf node_modules",
    "dev": "rollup -cw",
    "prebuild": "yarn clean",
    "build": "nest build -p tsconfig.json",
    "format": "prettier \"./src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "dep:upgrade": "ncu -p yarn -f /^@ynm*/ -u",
    "release": "standard-version",
    "release:major": "yarn release --release-as major",
    "release:minor": "yarn release --release-as minor",
    "release:patch": "yarn release --release-as patch",
    "publish": "npm publish --access public"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.3",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/graphql": "^10.1.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^9.1.2",
    "@nestjs/platform-express": "^9.0.0",
    "apollo-server-express": "^3.10.3",
    "class-validator": "^0.13.2",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "http-status-codes": "^1.3.2",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.21",
    "qs": "^6.7.0",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@types/http-status-codes": "^1.2.0",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/lodash": "^4.14.185",
    "@types/qs": "^6.5.3",
    "cz-conventional-changelog": "3.0.2",
    "jwt-simple": "^0.5.6"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHead": "0c074c57d7aece821897e58117231d175f908081"
}
