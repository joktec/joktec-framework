{
  "name": "@baotg/mysql",
  "description": "JobHopin - MySql Service",
  "version": "1.0.4",
  "private": false,
  "license": "UNLICENSED",
  "author": "JobHopin",
  "main": "dist/index",
  "types": "dist/index",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "chmod u+x ./bin/*",
    "clean": "rm -rf dist",
    "purge": "rm -rf node_modules",
    "dev": "nest start --watch -p tsconfig.json",
    "prebuild": "yarn clean",
    "build": "nest build -p tsconfig.json",
    "format": "prettier \"./src/**/*.ts\" --ignore-path ./.prettierignore --write",
    "lint": "eslint --fix \"./src/**/*.ts\"",
    "dep:upgrade": "ncu -p yarn -f /^@baotg*/ -u",
    "gen:entity": "./bin/generate-entity.ts && yarn format && yarn lint"
  },
  "dependencies": {
    "@baotg/core": "1.0.4",
    "knex": "^2.3.0",
    "knex-schema-inspector": "^3.0.0",
    "mysql": "2.18.1",
    "mysql2": "^2.3.3",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/knex": "^0.16.1",
    "@types/mysql": "^2.15.17"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gitHead": "d223bf26bfdc701b397b0694a3b54b691185fa80"
}
